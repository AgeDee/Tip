<?import javafx.scene.web.WebView?>

<AnchorPane prefHeight="616.0" prefWidth="568.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.mainController">
